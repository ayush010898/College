def Reverse(string):
    list1 = string.split(' ')
    list1.reverse()
    list1 = ' '.join(list1)
    return list1

def reverseChar(s): 
  str = "" 
  for i in s: 
    str = i + str
  return str

phrase = input("enter string ")
reverseString = Reverse(phrase)
print(reverseString) 
print ("The reversed string is ") 
print (reverseChar(reverseString))
